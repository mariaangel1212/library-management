{
    "version": 2,
    "framework": null,
    "functions": {
      "api/index.php": { "runtime": "vercel-php@0.6.0" }
    },
    "build": {
      "env": {
        "VERCEL_PHP_BUILDPACK_INSTALL_COMMAND": "bash build.sh"
      }
    },
    "builds": [
      {
        "src": "public/index.php",
        "use": "@vercel/php"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/public/index.php"
      }
    ],
    "env": {
      "APP_ENV": "production",
      "APP_DEBUG": "false",
      "APP_URL": "https://library-management-iytd.vercel.app",
      "APP_KEY": "base64:Xy6C46Ag72Zh4cEiqnv/BEBrU2EOvuuqmgr7I2TwQec=",
      "APP_CONFIG_CACHE": "/tmp/config.php",
      "APP_EVENTS_CACHE": "/tmp/events.php",
      "APP_PACKAGES_CACHE": "/tmp/packages.php",
      "APP_ROUTES_CACHE": "/tmp/routes.php",
      "APP_SERVICES_CACHE": "/tmp/services.php",
      "VIEW_COMPILED_PATH": "/tmp",
      "CACHE_DRIVER": "array",
      "LOG_CHANNEL": "stderr",
      "SESSION_DRIVER": "cookie"
    }
  }
  